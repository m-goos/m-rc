{
  "name": "m-rc.nl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-open": "npm run clean && npm run build && npm run serve-build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write --cache .",
    "prettier:ci": "prettier --check .",
    "check-types": "tsc --noEmit",
